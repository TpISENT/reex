{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "^1.24",
        "drupal/auto_entitylabel": "^2.1",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/captcha": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/eck": "^1.0",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/fontawesome": "^2.8",
        "drupal/geolocation": "^2.0",
        "drupal/honeypot": "^1.29",
        "drupal/inline_entity_form": "^1.0",
        "drupal/mailsystem": "^4.1",
        "drupal/metatag": "^1.7",
        "drupal/multiple_registration": "^2.6",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/pbf": "^1.2",
        "drupal/profile": "^1.0",
        "drupal/rabbit_hole": "^1.0",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.3",
        "drupal/redirect_after_login": "^2.3",
        "drupal/rules": "^3.0",
        "drupal/smtp": "^1.0",
        "drupal/taxonomy_manager": "^1.0",
        "drupal/token": "^1.5",
        "drupal/webform": "^5.0",
        "drupal/webform_analysis": "^1.0",
        "drupal/webform_composite": "^1.0",
        "drupal/webform_encrypt": "^1.0",
        "drupal/webform_invitation": "^1.0",
        "drupal/webform_rest": "^2.0",
        "drupal/webform_views": "^5.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
